{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "cd ../shared && pnpm build",
    "dev": "vite dev",
    "dev:spa": "vite --config vite.spa.config.ts",
    "build": "vite build",
    "build:spa": "vite build --config vite.spa.config.ts",
    "start": "node dist/server/server.js",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:production": "playwright test tests/orders-production.spec.ts",
    "test:production:har": "RECORD_HAR=true playwright test tests/orders-production.spec.ts",
    "test:network": "playwright test tests/orders-production.spec.ts --grep 'Network'",
    "test:report": "playwright show-report playwright-report",
    "browse": "playwright test tests/browse.spec.ts --headed --project=chromium",
    "browse:returns": "PAGE=/returns playwright test tests/browse.spec.ts --headed --project=chromium"
  },
